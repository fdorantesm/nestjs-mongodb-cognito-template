{
  "name": "nestjs-mongodb-cognito-template",
  "version": "0.0.1",
  "description": "A NestJS template project with MongoDB and Cognito integration for rapid development.",
  "author": "Fernando Dorantes <fernando@dorant.es>",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seed:roles": "ts-node -r tsconfig-paths/register scripts/seeds/roles.seed.ts",
    "seed:roles:unseed": "ts-node -r tsconfig-paths/register scripts/seeds/roles.unseed.ts",
    "seed:roles:reset": "yarn run seed:roles:unseed && yarn run seed:roles",
    "seed:settings": "ts-node -r tsconfig-paths/register scripts/seeds/settings.seed.ts",
    "seed:all": "yarn run seed:roles && yarn run seed:users && yarn run seed:settings",
    "seed:all:undo": "yarn run seed:users:unseed && yarn run seed:roles:unseed",
    "seed:all:reset": "yarn run seed:all:undo && yarn run seed:all",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:benchmark": "NODE_ENV=test node --expose-gc ./node_modules/.bin/jest --config ./test/jest-e2e.json --testPathPattern=benchmark-simple.e2e-spec.ts --runInBand --forceExit",
    "test:tokens": "ts-node -r tsconfig-paths/register scripts/test-tokens.ts",
    "seed:users": "ts-node -r tsconfig-paths/register scripts/seeds/users.seed.ts",
    "seed:users:local": "ts-node -r tsconfig-paths/register scripts/seeds/users-local.seed.ts",
    "seed:users:unseed": "ts-node -r tsconfig-paths/register scripts/seeds/users.unseed.ts",
    "seed:users:reset": "yarn run seed:users:unseed && yarn run seed:users",
    "seed:settings:reset": "yarn run seed:settings && yarn run seed:settings"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.777.0",
    "@aws-sdk/client-cognito-identity-provider": "3.777.0",
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.1",
    "@nestjs/cqrs": "11.0.3",
    "@nestjs/mongoose": "11.0.3",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.1",
    "@nestjs/swagger": "11.1.0",
    "@ts-ddd/state-machine": "1.0.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "connection-string": "4.4.0",
    "express-rate-limit": "7.5.0",
    "helmet": "8.1.0",
    "http-status-codes": "2.3.0",
    "joi": "17.13.3",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.2.0",
    "mongoose": "8.13.1",
    "mongoose-anonymoose": "1.0.1",
    "mongoose-paginate-v2": "1.9.1",
    "mongoose-query-parser": "1.3.1",
    "nestjs-uuid": "0.1.4",
    "passport": "0.7.0",
    "ramda": "0.30.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@nestjs/cli": "11.0.0",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.1",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.10.7",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/nedb": "1.8.16",
    "@types/node": "22.10.7",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-hexagonal-architecture": "1.0.3",
    "eslint-plugin-prettier": "5.2.2",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "light-my-request": "6.6.0",
    "nedb-promises": "6.2.3",
    "prettier": "3.4.2",
    "source-map-support": "0.5.21",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": [
        "<rootDir>/$1",
        "<rootDir>/../src/$1"
      ]
    }
  }
}
